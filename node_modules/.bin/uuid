#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../@expo/configure-splash-screen/node_modules/@react-native-community/cli-platform-ios/node_modules/uuid/bin/uuid" "$@"
  ret=$?
else 
  node  "$basedir/../@expo/configure-splash-screen/node_modules/@react-native-community/cli-platform-ios/node_modules/uuid/bin/uuid" "$@"
  ret=$?
fi
exit $ret
